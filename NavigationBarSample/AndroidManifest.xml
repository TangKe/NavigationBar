<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.tangke.navigationbarsample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="me.tangke.navigationbarsample.DemoBundleActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="me.tangke.navigationbardsample.activity.NavigationAttribute"
            android:label="@string/title_navigation_bar_attribute" >
            <intent-filter>
                <action android:name="me.tangke.DEMO" />
            </intent-filter>
        </activity>
        <activity
            android:name="me.tangke.navigationbardsample.activity.NavigationFeature"
            android:label="@string/title_navigation_bar_attribute" >
        </activity>
    </application>

</manifest>